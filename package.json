	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean": "rimraf build",
		"lint": "eslint **/*.js",
		"build:dev":
			"npm run lint && npm run clean && webpack --config webpack.dev.js",
		"build:prod":
			"npm run lint && npm run clean && webpack --config webpack.prod.js",
		"watch:client":
			" npm run clean && webpack --config webpack.dev.js --watch --progress",
		"watch:server":
			"nodemon --inspect-brk=0.0.0.0:9229 index.js --ignore 'build/'",
		"start": "npm-run-all --parallel watch:server watch:client"
	},
